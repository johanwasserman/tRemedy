<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
	<HEADER 
		PLATEFORM="ALL" 
		SERIAL="" 
		VERSION="0.397" 
		STATUS="ALPHA"
		COMPATIBILITY="ALL" 
		AUTHOR="johan.wasserman@gmail.com" 
		RELEASE_DATE="20110229A"
		STARTABLE="false" 
		HAS_CONDITIONAL_OUTPUTS="true"
		DATA_AUTO_PROPAGATE="false"
		SCHEMA_AUTO_PROPAGATE ="true"
		PARTITIONING="AUTO"
		>
		<SIGNATURE />
	</HEADER>

	<FAMILIES> 
		<FAMILY>Business/BMC Remedy</FAMILY> 
	</FAMILIES>  
	<DOCUMENTATION> 
		<URL>http://www.sxsesoftware.com</URL>
		<!--<URL>http://www.sxsesoftware.com/talend/components/tRemedyAttachment.html</URL>--> 
	</DOCUMENTATION>  
	<CONNECTORS> 
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MIN_INPUT="1" MAX_OUTPUT="0"/> 
		<CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0"	MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS> 
		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="1" GROUP="CONNECTION">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" FILTER="REGEXP:^(.)*(tRemedyConnection)(.)*" NUM_ROW="2" SHOW_IF="USE_EXISTING_CONNECTION == 'true'" GROUP="CONNECTION" />

		<PARAMETER NAME="ARSERVER" FIELD="TEXT" REQUIRED="true" NUM_ROW ="10" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
      		<DEFAULT>"127.0.0.1"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PORT" FIELD="TEXT" REQUIRED="false" NUM_ROW ="10" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
      		<DEFAULT>"0"</DEFAULT>
		</PARAMETER>
    	<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true" NUM_ROW ="11" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
      		<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PASSWORD" FIELD="TEXT" REQUIRED="false" NUM_ROW ="11" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
      		<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="FORM" FIELD="TEXT" REQUIRED="true" NUM_ROW ="3" >
      		<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="10" SHOW="false">
			<DEFAULT/>
		</PARAMETER>
  		<PARAMETER NAME="DELETE_ID_FIELD" FIELD="PREV_COLUMN_LIST" NUM_ROW="35" REQUIRED="true">
		</PARAMETER>
	    <PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="60" CONTEXT="REJECT" SHOW="true">
			<TABLE READONLY="true">
			    <COLUMN NAME="ETL_DELETE_ERR" TYPE="id_String" READONLY="true" CUSTOM="true"/>
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="LOG4J_ACTIVATE" FIELD="CHECK" REQUIRED="false" NUM_ROW="70">
  			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS> 
	
	</ADVANCED_PARAMETERS>  
	
	<CODEGENERATION> 
		<IMPORTS> 
			<IMPORT MODULE="arapi81_build001.jar" NAME="arapi81_build001" REQUIRED="true"/>  
			<IMPORT MODULE="log4j-1.2.14.jar" NAME="log4j" REQUIRED="true"/>
		</IMPORTS> 
	</CODEGENERATION>  
	
	<RETURNS> 
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/> 
	</RETURNS> 
</COMPONENT>
