<COMPONENT>

	<HEADER 
		PLATEFORM="ALL" 
		SERIAL="" 
		VERSION="0.738" 
		STATUS="ALPHA"
		COMPATIBILITY="ALL" 
		AUTHOR="johan.wasserman@gmail.com" 
		RELEASE_DATE="20070312A"
		STARTABLE="false" 
		HAS_CONDITIONAL_OUTPUTS="true"
		DATA_AUTO_PROPAGATE="false"
		SCHEMA_AUTO_PROPAGATE ="true"
		PARTITIONING="AUTO"
		>
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Business/BMC Remedy</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL>http://www.sxsesoftware.com</URL>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR NAME="FILTER" CTYPE="FLOW" MAX_OUTPUT="1" COLOR="086438" BASE_SCHEMA="FLOW" />
		<CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="ff0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="1" GROUP="CONNECTION">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" FILTER="REGEXP:^(.)*(tRemedyConnection)(.)*" NUM_ROW="2" SHOW_IF="USE_EXISTING_CONNECTION == 'true'" GROUP="CONNECTION" />

		<PARAMETER NAME="ARSERVER" FIELD="TEXT" REQUIRED="true" NUM_ROW ="10" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
      		<DEFAULT>"127.0.0.1"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PORT" FIELD="TEXT" REQUIRED="false" NUM_ROW ="10" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
      		<DEFAULT>"0"</DEFAULT>
		</PARAMETER>
    	<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true" NUM_ROW ="11" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
      		<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" REQUIRED="false" NUM_ROW ="11" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
      		<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FORM" FIELD="TEXT" REQUIRED="true" NUM_ROW ="20" >
      		<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="20"/>
		<PARAMETER NAME="USEMERGE" FIELD="CHECK" REQUIRED="false" NUM_ROW="30">
  			<DEFAULT>false</DEFAULT>
		</PARAMETER>					
		<PARAMETER NAME="HANDLEID" FIELD="CLOSED_LIST" REQUIRED="true" NUM_ROW="40" SHOW_IF="USEMERGE=='true'">
			<ITEMS DEFAULT="DUP_ERROR">
				<ITEM NAME="DUP_ERROR" VALUE="AR_MERGE_ENTRY_DUP_ERROR" />
				<ITEM NAME="DUP_NEW_ID" VALUE="AR_MERGE_ENTRY_DUP_NEW_ID" />
				<ITEM NAME="DUP_OVERWRITE" VALUE="AR_MERGE_ENTRY_DUP_OVERWRITE" />
				<ITEM NAME="DUP_MERGE" VALUE="AR_MERGE_ENTRY_DUP_MERGE" />
				<ITEM NAME="GEN_NEW_ID" VALUE="AR_MERGE_ENTRY_GEN_NEW_ID" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="NULLOK" FIELD="CHECK" REQUIRED="false" NUM_ROW="50"  SHOW_IF="USEMERGE=='true'">
  			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PATTERN" FIELD="CHECK" REQUIRED="false" NUM_ROW="51"  SHOW_IF="USEMERGE=='true'">
  			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="WORKFLOW" FIELD="CHECK" REQUIRED="false" NUM_ROW="52"  SHOW_IF="USEMERGE=='true'">
  			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NULLEMPTY" FIELD="CHECK" REQUIRED="false" NUM_ROW="53">
  			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="LOG4J_ACTIVATE" FIELD="CHECK" REQUIRED="false" NUM_ROW="60">
  			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_FILTER" FIELD="SCHEMA_TYPE" NUM_ROW="700" CONTEXT="FILTER">
		  <TABLE READONLY="true">
		    <COLUMN NAME="ETL_IMPORT_REF" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
		  </TABLE>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE" NUM_ROW="71" CONTEXT="REJECT">
		  <TABLE READONLY="true">
		    <COLUMN NAME="ETL_IMPORT_ERR" TYPE="id_String" READONLY="true" CUSTOM="true"/>
		  </TABLE>
		</PARAMETER>
	</PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="arapi801" MODULE="arapi81_build001.jar" REQUIRED="true" />
			<IMPORT MODULE="log4j-1.2.14.jar" NAME="log4j" REQUIRED="true"/>
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
		<RETURN NAME="NB_LINE_OK" TYPE="id_Integer"	AVAILABILITY="AFTER"/>
		<RETURN NAME="NB_LINE_REJECT" TYPE="id_Integer"	AVAILABILITY="AFTER"/>
	</RETURNS>
</COMPONENT>